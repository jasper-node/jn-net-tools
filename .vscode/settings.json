{
  "editor.formatOnSave": true,
  "deno.enable": true,
  "deno.disablePaths": [
    "frontend"
  ],
  "deno.config": "deno.json",
  "deno.lint": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.testing.args": [
    "-A"
  ],
  "deno.envFile": ".env",
  "cSpell.words": [
    "conn",
    "jnnt"
  ]
}
